{% extends "PHPMBundle:Creneau:layout.html.twig"  %}
{% set pageTitle = 'Nouveau Cr√©neau' %}
{% use "PHPMBundle::show.html.twig" %}

{% block content %}
<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Debut</th>
            <td>{{ entity.debut|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Fin</th>
            <td>{{ entity.fin|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>
{% endblock %}