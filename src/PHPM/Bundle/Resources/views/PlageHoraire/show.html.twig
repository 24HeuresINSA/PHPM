{% extends "::base.html.twig"  %}

{% block title %}
	Plages horaire -> D&eacute;tails de la plage horaire
{% endblock %}

{% block content %}
<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Debut</th>
            <td>{{ entity.debut|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Fin</th>
            <td>{{ entity.fin|date('Y-m-d H:i:s') }}</td>
        </tr>
         <tr>
            <th>T&acirc;che</th>
            <td>{{ entity.tache }}</td>
        </tr>
         <tr>
            <th>NbOrgasNecessaires</th>
            <td>{{ entity.nbOrgasNecessaires }}</td>
        </tr>
    </tbody>
</table>
{% endblock %}

{% block menu_action %}
<ul class="record_actions">
    <li>
        <a href="{{ path('plagehoraire') }}">
            Revenir &agrave; la liste
        </a>
    </li>
    <li>
        <a href="{{ path('plagehoraire_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
    	<a href="javascript:confirm_delete('{{ path('plagehoraire_delete', { 'id': entity.id }) }}')">Supprimer</a>
    </li>
</ul>
{% endblock %}