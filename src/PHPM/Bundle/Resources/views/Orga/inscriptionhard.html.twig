{% extends "PHPMBundle:Orga:layout.html.twig"  %}
{% set pageTitle = 'Mes Disponibilit√©s' %}

{% form_theme form _self %}

{% block style %}
{{ parent() }}
			{% stylesheets 'bundles/phpm/css/registerHard.css' filter='cssrewrite'  %}
				<link rel="stylesheet" href="{{ asset_url }}" type="text/css" media="screen" />
			{% endstylesheets %}
{% endblock %}


{% block checkbox_widget %}
{% spaceless %}
<br/><input type="checkbox" {{ block('widget_attributes') }}{% if value is defined %} value="{{ value }}"{% endif %}{% if checked %} checked="checked"{% endif %} />
{% endspaceless %}
{% endblock checkbox_widget %}


{% block content %}
<form action="{{ path('orga_inscriptionhard') }}" method="post" {{ form_enctype(form) }}>
{{ form_widget(form) }}
    <p>
        <button type="submit">Edit</button>
    </p>
</form>




{% endblock %}

{% block actions %}
{% endblock %}
